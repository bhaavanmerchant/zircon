source_set("ldso") {
  configs -= [ "$zx/public/gn/config:user" ]
  configs += [ "$zx/third_party/ulib/musl:musl_config" ]
  deps = [
    "$zx/system/ulib/ldmsg",
  ]
  sources = [
    "$zx/third_party/ulib/musl/arch/${toolchain.cpu}/dl-entry.S",
    "dlstart.c",
    "dynlink-sancov.S",
    "dynlink.c",
  ]
}
