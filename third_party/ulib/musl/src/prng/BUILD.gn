source_set("prng") {
  deps -= [ "$zx/public/gn/config:user" ]
  deps += [ "$zx/third_party/ulib/musl:musl_config" ]
  sources = [
    "__rand48_step.c",
    "__seed48.c",
    "drand48.c",
    "lcong48.c",
    "lrand48.c",
    "mrand48.c",
    "rand.c",
    "rand_r.c",
    "random.c",
    "seed48.c",
    "srand48.c",
  ]
}
