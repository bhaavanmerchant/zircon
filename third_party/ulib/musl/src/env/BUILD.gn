source_set("env") {
  configs -= [ "$zx/public/gn/config:user" ]
  configs += [ "$zx/third_party/ulib/musl:musl_config" ]
  sources = [
    "__environ.c",
    "__libc_start_main.c",
    "__stack_chk_fail.c",
    "clearenv.c",
    "getenv.c",
    "putenv.c",
    "setenv.c",
    "unsetenv.c",
  ]
}
