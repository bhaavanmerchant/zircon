    "main.c",
    "reader_tests.cpp",
    "records_tests.cpp",
  ]
  deps += [
    "$zx/system/ulib/fbl",
    "$zx/system/ulib/trace-engine",
    "$zx/system/ulib/trace-reader",
    "$zx/system/ulib/unittest",
