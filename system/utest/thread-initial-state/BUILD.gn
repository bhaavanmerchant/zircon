test("thread-initial-state") {
  sources = [
    "init-state.c",
  ]
  if (current_cpu == "x64") {
    sources += [ "x86-64.S" ]
  } else {
    sources += [ "${zircon_cpu}.S" ]
  }
  deps += [
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/unittest",
    "$zx/system/ulib/zircon",
  ]
}
