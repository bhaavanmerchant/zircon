group("board") {
  deps = [
    "machina",
    "qemu",
  ]
  if (current_cpu == "arm64") {
    deps += [
      "astro",
      "gauss",
      "hikey960",
      "imx8mevk",
      "imx8mmevk",
      "mt8167s_ref",
      "sherlock",
      "vim",
      "vim-machina",
    ]
  }
}
