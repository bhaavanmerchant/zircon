library("pretty") {
  kernel = true
  host = true
  static = true
  sources = [
    "hexdump.c",
    "sizes.c",
  ]
  if (!is_kernel && is_fuchsia) {
    deps += [
      "$zx/system/ulib/c",
      "$zx/system/ulib/fdio",
    ]
  }
}
