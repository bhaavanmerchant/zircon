library("inet6") {
  sources = [
    "checksum.c",
    "eth-client.c",
    "inet6.c",
    "netifc.c",
  ]
  configs += [ "$zx/public/gn/config:visibility_hidden" ]
  deps = [
    "$zx/system/fidl/zircon-ethernet:c",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/zircon",
    "$zx/system/ulib/zircon-internal",
  ]
}
